/**
 *  Exclusive-or gate.  out = a xor b.
 */

CHIP Xor {

    IN  a, b;
    OUT out;

    PARTS:
    Not(in=a, out=notA);
    Not(in=b, out=notB);
    And(a=a, b=notB, out=val1);
    And(a=notA, b=b, out=val2);
    Or(a=val1, b=val2, out=out);
    // a.!b + !a.b
}
